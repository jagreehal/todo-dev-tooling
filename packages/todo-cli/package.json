{
  "name": "@jagreehal/todo-cli",
  "version": "1.0.7",
  "main": "index.js",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "ts-node todo-cli.ts",
    "debug": "node --inspect-brk -r ts-node/register todo-cli.ts",
    "build": "tsc",
    "package": "ncc build src/index.ts -o build && pkg build/index.js -c package.json",
    "start": "DEBUG=ToDoSDK node dist/index.js"
  },
  "bin": {
    "todo-cli": "./dist/index.js"
  },
  "dependencies": {
    "@jagreehal/todo-sdk": "workspace:*",
    "commander": "^11.0.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@types/node": "^20.6.2",
    "@vercel/ncc": "^0.38.0",
    "pkg": "^5.8.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "pkg": {
    "targets": [
      "node14-linux-x64",
      "node14-macos-x64",
      "node14-macos-arm64",
      "node14-win-x64"
    ],
    "outputPath": "clis",
    "assets": [
      "node_modules"
    ]
  }
}
